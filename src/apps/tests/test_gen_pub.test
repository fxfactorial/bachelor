#!/bin/sh

# N = 11 * 13
# e = 103
# d = 7
./gen pub -N 143 -e 103 | grep -q PUBLIC
if [ $? -ne 0 ]
then
    echo "should accept public exponent and public modulus."
    exit 1
fi


